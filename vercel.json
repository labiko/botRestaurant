{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "botResto/**"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/index.html" }
  ],
  "outputDirectory": "botResto/www"
}